<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.musicociel" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Musicociel</name>
    <description>Application allowing to display songs.</description>
    <author email="divde@musicociel.fr">DivDE</author>
    <hook src="cordova-prepare.js" type="after_prepare" />
    <content src="index.cordova.html" />
    <access origin="*" />
    <icon src="node_modules/@musicociel/webapp/build/output/icon.png" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-sqlite-2" spec="^1.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
